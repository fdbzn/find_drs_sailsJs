            return t;
        }(document, "script", "twitter-wjs"));</script>

        <!--SCRIPTS-->
        <script src="/js/dependencies/sails.io.js"></script>
        <script src="/js/dependencies/bloodhound.min.js"></script>
        <script src="/js/dependencies/typeahead.bundle.min.js"></script>
        <script src="/js/dependencies/typeahead.jquery.min.js"></script>
        <script src="/js/animateBGColor.js"></script>
        <script src="/js/local_dependencies/AutocompleteField.js"></script>
        <script src="/js/local_dependencies/GtagTracking.js"></script>
        <script src="/js/local_dependencies/template_helper.js"></script>
        <script src="/js/main.js"></script>
        <script src="/js/map.js"></script>
        <script src="/js/pages/createHCenter.js"></script>
        <script src="/js/pages/doctor_profile.js"></script>
        <script src="/js/pages/health_center_detail.js"></script>
        <script src="/js/pages/welcome.js"></script>
        <script src="/js/search_map.js"></script>
        <script src="/js/star-rating.min.js"></script>
        <script src="/js/theme.js"></script>
        <script src="/js/tpl/create_hc_tpl.js"></script>
        <script src="/js/tpl/doctor_comments.js"></script>
        <script src="/js/tpl/tplElement.js"></script>
        <script src="/js/typeahead.jquery.min.js"></script>
        <script src="/js/yiPage.js"></script>
        <!--SCRIPTS END-->

        <% if(typeof scriptJS != 'undefined'){ %>
        <% scriptJS.forEach(function(script){ %>
        <script src="js/pages/<%=script%>.js"></script>
        <% }); %>
        <% } %>

        <% if( typeof req.user != 'undefined' ){ %>
            <script>
                initUserSession();
            </script>
        <% }else{%>
            <script>
                userSession = {noSession:true}
            </script>
        <% } %>

        <script async defer
                src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCvcw_T-pqS6QIPCuh8aO_KNIZ860Gou4w&libraries=places&callback=initSearchMap">
        </script>
